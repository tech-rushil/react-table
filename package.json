{
    "name": "react-app",
    "version": "0.1.0",
    "private": true,
    "homepage": "https://skeleton.com",
    "dependencies": {
        "antd": "^4.14.1",
        "axios": "^0.19.0",
        "cors": "^2.8.5",
        "crypto-js": "^4.0.0",
        "history": "^4.10.1",
        "moment": "^2.29.1",
        "moment-timezone": "^0.5.31",
        "query-string": "^6.9.0",
        "react": "^16.11.0",
        "react-dom": "^16.11.0",
        "react-helmet": "^6.1.0",
        "react-html-parser": "^2.0.2",
        "react-player": "^2.6.0",
        "react-redux": "^7.1.3",
        "react-router-dom": "^5.1.2",
        "react-scripts": "^3.4.0",
        "redux": "^4.0.5",
        "slugify": "^1.4.0"
    },
    "scripts": {
        "start": "source env/local_env.sh && react-scripts start",
        "dev": "env-cmd -f env/local.env react-scripts start",
        "build": "env-cmd -f env/prod.env react-scripts build",
        "build-dev": "env-cmd -f env/dev.env react-scripts build",
        "workbox-wizard": "workbox wizard",
        "workbox-generate": "workbox generateSW workbox-config.js",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": "react-app",
        "env": {
            "es6": true
        },
        "ignorePatterns": [
            "node_modules/"
        ],
        "parser": "babel-eslint",
        "parserOptions": {
            "ecmaVersion": 8
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@svgr/cli": "^4.3.3",
        "env-cmd": "^10.1.0",
        "eslint-plugin-import": "^2.20.1",
        "file-loader": "^5.0.2",
        "jwt-decode": "^2.2.0",
        "url-loader": "^3.0.0",
        "workbox-cli": "^5.1.3"
    }
}
